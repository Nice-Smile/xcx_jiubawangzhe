page {
    background-image: url("http://touzi.115xy.com/shezhibj.png");
    background-repeat: repeat;
    background-size: 100% 100%;
}

.ad,.touziwei {
    float: left;
    width: 100%;
}

.touziwei {
    height: 300rpx;
    margin-top: 15rpx;
}

.shu {
    border: 1px solid #000;
    border-radius: 40rpx;
}

.shu,.xuanzhong {
    float: left;
    height: 65rpx;
    line-height: 65rpx;
    margin-left: 10%;
    margin-top: 30rpx;
    text-align: center;
    width: 20%;
}

.xuanzhong {
    background-color: #000;
    border: 1px solid #fdd654;
    border-radius: 40rpx;
    color: #000;
    font-weight: 700;
}

.shuliang {
    float: left;
    height: 120rpx;
    margin-top: 20rpx;
    width: 100%;
}

.wenzi {
    background-color: rgba(0,0,0,.2);
    border: 1px solid #aaa;
    border-radius: 60rpx;
    color: #fff;
    float: left;
    font-size: 36px;
    height: 105rpx;
    line-height: 105rpx;
    text-align: center;
    width: 30%;
    z-index: -1;
}

.anniu {
    float: left;
    height: 120rpx;
    width: 120rpx;
    z-index: 3;
}

.anniu:active {
    opacity: .6;
}

.jian {
    float: left;
    margin-left: 20%;
    width: 120rpx;
}

.geshu,.jian {
    height: 120rpx;
}

.geshu {
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    margin-top: 5rpx;
    width: 600rpx;
}

.biaoti {
    height: 80rpx;
    margin-top: 5pt;
    width: 100%;
}

.tit {
    color: #fff;
    float: left;
    font-size: 36rpx;
    font-size: 24px;
    height: 80rpx;
    line-height: 80rpx;
    margin-left: 15%;
    width: 40%;
}

.queding,.tit {
    text-align: center;
}

.queding {
    background-color: #fdd654;
    border: 1px solid #fdd654;
    border-radius: 15rpx;
    color: #757373;
    float: right;
    height: 60rpx;
    line-height: 60rpx;
    margin-right: 5%;
    margin-top: 10rpx;
    width: 20%;
}

.hui {
    float: left;
    height: 80rpx;
    margin-left: 2%;
    margin-top: 5rpx;
    width: 80rpx;
}

.hui image {
    max-height: 100%;
    max-width: 100%;
}

.gengduo {
    float: left;
    height: auto;
    width: 100%;
}

.rmbiaoti {
    float: float;
    font-weight: 700;
    height: 65rpx;
    line-height: 65rpx;
    margin-left: 5%;
    width: 50%;
}

.meiyigexcx {
    float: left;
    margin-top: 5%;
    width: 33%;
}

.meiyigetu {
    float: left;
    height: auto;
    margin-left: 20%;
    width: 60%;
}

.meiyigetu image {
    border-radius: 30rpx;
    height: 100%;
    width: 100%;
}

.meiyigebiaoti {
    float: left;
    font-weight: 700;
    height: 50rpx;
    text-align: center;
    width: 100%;
}