page {
    background-image: url("http://touzi.115xy.com/zhi/zhibj.png");
    background-repeat: repeat;
    background-size: 100% 100%;
}

.main {
    float: left;
    width: 100%;
}

.yaoqing {
    border: 1px solid #fff;
    border-radius: 30px;
    float: left;
    height: 60rpx;
    margin-left: 5%;
    margin-top: 70rpx;
    text-align: center;
    width: 160rpx;
}

.yaoqing,.yaoqing button {
    color: #fff;
    line-height: 60rpx;
}

.yaoqing button {
    background: none;
    font-size: 15px;
    height: 100%;
    margin: 0;
    padding: 0;
    width: 100%;
    z-index: 1;
}

.touzi {
    height: 100%;
    margin-left: 5%;
    margin-top: 170rpx;
    width: 90%;
}

.touzi image {
    border-radius: 30rpx;
    width: 100%;
}

.er {
    margin-left: 5%;
    width: 100%;
}

.er,.zhijian {
    float: left;
    height: auto;
}

.zhijian {
    margin-top: 3%;
    width: 45%;
}

.er,.zhijian1 {
    float: left;
    height: auto;
}

.zhijian1 {
    margin-top: 3%;
    width: 24%;
}

.zhijian1 image {
    border-radius: 30rpx;
    height: 80%;
    width: 80%;
}

.zhijian image {
    border-radius: 30rpx;
    height: 100%;
    width: 100%;
}

.dengguang {
    float: left;
    height: auto;
    margin-left: 5%;
    margin-top: 3%;
    width: 40%;
}

.tu {
    height: 100%;
    width: 100%;
}

.san {
    height: auto;
    margin-top: 2%;
    width: 43%;
}

.ggad,.san {
    float: left;
    margin-left: 5%;
}

.ggad {
    margin-top: 4%;
    width: 90%;
}